; ModuleID = 'main'

define i64 @TestIfElse(i1 %x) {
entry:
  br i1 %x, label %then, label %else

then:                                             ; preds = %entry
  ret i64 5
  br label %merge

else:                                             ; preds = %entry
  ret i64 2
  br label %merge

merge:                                            ; preds = %else, %then
  ret i64 0
}

define void @main() {
entry:
  %calltmp = call i64 @TestIfElse(i1 true)
  %calltmp1 = call i64 @TestIfElse(i1 false)
  ret void
}

