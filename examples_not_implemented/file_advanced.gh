class File:

	fileDescriptor Int

	None write(msg String, len Int):
		__builtins__.write(fileDescriptor, msg, len)

	(Int, String) read(num Int):
		buffer := Char[num]
		bytes_read := __builtins__.read(fileDescriptor, buffer, num)
		return (bytes_read, buffer)

stdin := File(0)
stdout := File(1)


i := 0
while i < 10:
	bytes_read, output := stdin.read(100)
	stdout.write(output, bytes_read)
	i = i + 1
