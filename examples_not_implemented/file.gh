class File:

	fileDescriptor Int

	None write(msg String, len Int):
		__builtins__.write(fileDescriptor, msg, len)

	String read(num Int):
		buffer := Char[num]
		__builtins__.read(fileDescriptor, buffer, num)
		return buffer

stdin := File(0)
stdout := File(1)

// stdout.write("foooo")
stdout.write(stdin.read(100), 100)